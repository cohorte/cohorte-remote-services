<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ecf.identity.namespace">
      <namespace
            class="org.cohorte.ecf.provider.jabsorb.identity.JabsorbNamespace"
            description="Jabsorb Namespace"
            name="ecf.namespace.jabsorb">
      </namespace>
      <namespace
            class="org.cohorte.ecf.provider.jabsorb.identity.JabsorbRemoteServiceNamespace"
            description="Jabsorb Remote Service Namespace"
            name="ecf.namespace.jabsorb.remoteservice">
      </namespace>
   </extension>
   <extension
         point="org.eclipse.ecf.containerFactory">
      <containerFactory
            class="org.cohorte.ecf.provider.jabsorb.JabsorbContainerInstantiator"
            description="Jabsorb Container Instantiator"
            name="ecf.jabsorb"
            server="true">
      </containerFactory>
   </extension>
</plugin>
